    <h1>{{_i}}Default grid system <small>12 columns with a responsive twist</small>{{/i}}</h1>
  </div>

  <div class="row show-grid">
    <div class="span1">1</div>
    <div class="span1">1</div>
    <div class="span1">1</div>
    <div class="span1">1</div>
    <div class="span1">1</div>
